{
  "name": "bws-ux",
  "version": "0.0.3",
  "description": "UI components for BWS apps",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/index.min.js",
  "repository": "github.com/rabidpug/bws-ux",
  "author": "Matt Cuneo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/react": "^4.0.0-alpha.24",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.82.0",
    "flow-copy-source": "^2.0.2",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "polished": "^2.2.0",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "selectorator": "^3.3.0",
    "semver": "^5.5.1",
    "styled-components": "^4.0.0-beta.11.1",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {
    "polished": "^2.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "selectorator": "^3.3.0",
    "styled-components": "^4.0.0-beta.11.1"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "clean": "rimraf dist lib es",
    "build:umd": "cross-env BABEL_ENV=es webpack",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel -d lib src --ignore '**/*.test.*'",
    "build:es": "cross-env BABEL_ENV=es babel -d es src --ignore '**/*.test.*'",
    "build:flowcjs": "flow-copy-source -v -i '**/*.test.*' src lib",
    "build:flowes": "flow-copy-source -v -i '**/*.test.*' src es",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "run-p test:*",
    "test:lint": "cross-env NODE_ENV=test eslint \"src/**/*.{js,jsx}\"",
    "test:flow": "cross-env NODE_ENV=test flow",
    "prepare": "run-s clean test build"
  },
  "dependencies": {}
}
